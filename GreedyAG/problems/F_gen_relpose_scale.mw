<Group hide-input="false" hide-output="false" labelreference="L4127" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">R := getRotMatrix(&lt;x, y, z&gt;)*(x^2+y^2+z^2+1):
Acoeffs := eq-&gt;&lt;coeff(eq, lambda1)|coeff(eq, lambda2)|coeff(eq, s)|coeff(coeff(coeff(eq, lambda1, 0), lambda2, 0), s, 0)&gt;:
mm := [1, 1, 1, 2, 2, 2, 3, 3, 3, 4, 4, 4, 5, 5, 5, 6, 6, 6, 7, 7, 7]:
ii := [2, 3, 2, 3, 4, 4, 4, 5, 5, 5, 6, 5, 6, 7, 7, 7, 1, 1, 1, 1, 2]:
jj := [3, 4, 4, 4, 5, 5, 5, 6, 6, 6, 7, 7, 7, 1, 1, 1, 2, 2, 2, 3, 3]:
kk := [4, 5, 6, 5, 6, 7, 6, 7, 1, 7, 2, 2, 1, 2, 3, 2, 3, 4, 3, 4, 5]:
ll := [5, 6, 7, 6, 7, 1, 7, 1, 2, 1, 3, 3, 2, 3, 4, 3, 4, 5, 4, 6, 6]:
eqs := []:
for n from 1 to nops(mm) do
    m := mm[n]:
    t1 := cross(v[1][m]).f[1][m]+lambda1*f[1][m]:
    t2 := cross(v[2][m]).f[2][m]*s+lambda2*f[2][m]:
    i, j, k, l := ii[n], jj[n], kk[n], ll[n]:
    E := &lt;&lt;R.cross(t1)-cross(t2).R|R&gt;, &lt;R|ZeroMatrix(3)&gt;&gt;:
    expr1 := Vector([f[2][i], v[2][i]*s])^%T.E.Vector([f[1][i], v[1][i]]):
    expr2 := Vector([f[2][j], v[2][j]*s])^%T.E.Vector([f[1][j], v[1][j]]):
    expr3 := Vector([f[2][k], v[2][k]*s])^%T.E.Vector([f[1][k], v[1][k]]):
    expr4 := Vector([f[2][l], v[2][l]*s])^%T.E.Vector([f[1][l], v[1][l]]):
    A := map(expand, &lt;Acoeffs(expr1), Acoeffs(expr2), Acoeffs(expr3), Acoeffs(expr4)&gt;):
    eqs := [op(eqs), Determinant(A)]:
end do:</Text-field>
</Input>
</Group>
