<Group hide-input="false" hide-output="false" labelreference="L951" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nP := 7:
irnd := rand(-1000..1000)/1000:
for j from 1 to 2 do
    for i from 1 to nP do
        a, b := irnd(), irnd():
        c, d := irnd(), irnd():
        cosf, sinf := (a^2-b^2)/(a^2+b^2), 2*a*b/(a^2+b^2):
        cosp, sinp := (c^2-d^2)/(c^2+d^2), 2*c*d/(c^2+d^2):
        f[j][i] := &lt;cosf*cosp, cosf*sinp, sinf&gt;:
        v[j][i] := cross(f[j][i]).&lt;irnd(), irnd(), irnd()&gt;:
    end do:
end do:</Text-field>
</Input>
</Group>
