<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">irnd := rand(-1000..1000)/1000:
s := irnd():
for j from 1 to 2 do
    for i from 1 to 5 do
        a, b := irnd(), irnd():
        c, d := irnd(), irnd():
        cosf, sinf := (a^2-b^2)/(a^2+b^2), 2*a*b/(a^2+b^2):
        cosp, sinp := (c^2-d^2)/(c^2+d^2), 2*c*d/(c^2+d^2):
        x||j[i] := &lt;cosf*cosp, cosf*sinp, sinf&gt;:
        y||j[i] := cross(x||j[i]).&lt;irnd(), irnd(), irnd()&gt;:
    end do:
end do:</Text-field>
</Input>
</Group>
