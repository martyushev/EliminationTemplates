<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### procedure that computes Macaulay matrix of initial polynomial system
C_gen5pra := proc({symbolic:=1})
    local _name, _size, AA, eqs, vars, C0, V0, V, cth, sth, tx, r, E:
    _name, _size := [`x1`, `x2`, `s`], [[2,4], [2,4], [1,1]]:
    if symbolic=1 then
        AA := seq(Matrix(op(_size[i]), (x,y)-&gt;_name[i](x+(y-1)*_size[i][1])), i=1..nops(_name)):
    else
        AA := seq(rndMat(op(_size[i])), i=1..nops(_name)-1), &lt;&lt;arccos(3/5)&gt;&gt;:
    end if:
    
    eqs := map(expand, eqs):
    if symbolic=0 then return eqs: end if:
    vars := indets(eqs) minus indets([AA]):
