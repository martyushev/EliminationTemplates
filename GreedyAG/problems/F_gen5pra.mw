<Group hide-input="false" hide-output="false" labelreference="L1305" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">R := (2*s^2-1)*IdentityMatrix(3)+2*(&lt;u, v, w&gt;.Transpose(&lt;u, v, w&gt;)-s*cross(&lt;u, v, w&gt;)):
Acoeffs := p-&gt;&lt;coeff(p, lambda1)|coeff(p, lambda2)|coeff(coeff(p, lambda1, 0), lambda2, 0)&gt;:
ind := combinat[choose]<Font opaque="false" subscript="false" underline="false" superscript="false" bold="true" foreground="[255,0,0]" italic="false" executable="true" selection-placeholder="false" size="12" readonly="false" background="[255,255,255]" style="Help Normal" placeholder="false" family="Monospaced" font_style_name="Help Normal"> </Font>([$ 1..5-1], 3);
mm := [1, 2, 3, 4, 5]:
ii := [2, 3, 4, 5, 1]:
jj := [3, 4, 5, 1, 2]:
kk := [4, 5, 1, 2, 3]:
for m from 1 to 5 do
    t1 := cross(Y1[..,m]).X1[..,m]+lambda1*X1[..,m]:
    t2 := cross(Y2[..,m]).X2[..,m]+lambda2*X2[..,m]:
    i, j, k := ii[m], jj[m], kk[m]:
    expr1 := Transpose(X2[..,i]).R.Y1[..,i]+Transpose(X1[..,i]).Transpose(R).Y2[..,i]+Transpose(X2[..,i]).(R.cross(t1)-cross(t2).R).X1[..,i]:
    expr2 := Transpose(X2[..,j]).R.Y1[..,j]+Transpose(X1[..,j]).Transpose(R).Y2[..,j]+Transpose(X2[..,j]).(R.cross(t1)-cross(t2).R).X1[..,j]:
    expr3 := Transpose(X2[..,k]).R.Y1[..,k]+Transpose(X1[..,k]).Transpose(R).Y2[..,k]+Transpose(X2[..,k]).(R.cross(t1)-cross(t2).R).X1[..,k]:
    A := &lt;Acoeffs(expr1), Acoeffs(expr2), Acoeffs(expr3)&gt;:
    eq[m] := Determinant(A):
end do:
eq[6] := u^2+v^2+w^2+s^2-1:
eqs := [seq(eq[i], i=1..6)]:</Text-field>
