    local fd := fopen(cat(&quot;nongb/nongb_&quot;, prob, &quot;.txt&quot;), WRITE):
    map(x-&gt;<Font encoding="UTF-8">fprintf(fd, &quot;%a\134n&quot;, x)</Font>, QBs):
    fclose(fd):
end proc:</Text-field>
</Input>
