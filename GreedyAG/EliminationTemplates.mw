    Ws := [op({seq(getTord(GBs[i], Groebner[LeadingMonomial](GBs[i], Ts[i]))[1], i=1..nops(GBs))})]:
    Ws := map(z-&gt;zip((x, y)-&gt;x*y, z, wt), Ws):
    map(x-&gt;wdeg(x, vars), Ws):
end proc:</Text-field>
