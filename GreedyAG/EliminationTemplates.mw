    end proc:
    H0 := map(expand, C.CM) mod pp:
    uH := Matrix(nG, nF, (i, j)-&gt;undetPolyni(H0[i, j], vars, i, j)):
    pars := {seq(_u[i], i=1..nops(indets(uH) minus vars))}:
    leqs := map(coeffs, {seq(expand(-G[i] + add(uH[i, j]*F[j], j=1..nF)) mod pp, i=1..nG)}, vars):
