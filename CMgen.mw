    local F, i, prob1, pp, Ts, CMs, vars, q, m, fcm, j:
    read cat(&quot;problems/F_&quot;, prob, &quot;.m&quot;):
    F := eval(cat(F_, prob)):
    pp, Ts, CMs := readCM(prob, px):
    Ts := map(x-&gt;StringTools[SubstituteAll](StringTools[SubstituteAll](convert(op(1, x), string), &quot;[&quot;, &quot;(&quot;), &quot;]&quot;, &quot;)&quot;), Ts):
    CMs := map(x-&gt;StringTools[SubstituteAll](StringTools[SubstituteAll](convert(convert(x^%T, listlist), string), &quot;[&quot;, &quot;{&quot;), &quot;]&quot;, &quot;}&quot;), CMs):
    vars := [op(indets(F))]:
    q := iquo(nops(Ts), N, 'r'):
    m := ifelse(r &lt;&gt; 0, q + 1, q):
    for i from 1 to N do
        prob1 := cat(prob, &quot;_&quot;, i):
        cat(F_, prob1) := F:
        save cat(F_, prob1), cat(&quot;cluster/problems/F_&quot;, prob1, &quot;.m&quot;):
        fcm := fopen(cat(&quot;cluster/cm/cm_&quot;, prob1, &quot;.txt&quot;), WRITE):
        <Font encoding="UTF-8">fprintf(fcm, &quot;%a\134n%a\134n&quot;, pp, vars</Font>):
        for j from 1+(i-1)*m to min(i*m, nops(Ts)) do
            <Font encoding="UTF-8">fprintf(fcm, &quot;%s\134n&quot;, Ts[j]):</Font>
            <Font encoding="UTF-8">fprintf(fcm, &quot;matrix %s\134n&quot;, CMs[j]):
</Font>        end do:
        fclose(fcm):
    end do:
end proc:</Text-field>
</Input>
</Group>
