<Group hide-input="false" hide-output="false" labelreference="L3898" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nP := 7:
rR := getRotMatrix(rndVec(3)):
tau := Trace(rR):
vC := rndVec(3):
rt := -rR.vC:
rK := &lt;&lt;10/11|0|1/3&gt;, &lt;0|10/11|-2/5&gt;, &lt;0|0|1&gt;&gt;:
rP1, rP2 := rK.&lt;IdentityMatrix(3)|&lt;0, 0, 0&gt;&gt;, rK.&lt;rR|rt&gt;:
for i from 1 to nP do
    XQ[i] := &lt;rndVec(3), 1&gt;:
end do:
for i from 1 to nP do
    for j from 1 to 2 do
        t := convert(rP||j.XQ[i], Vector):
        x[j][i] := t[1]:
        y[j][i] := t[2]:
        z[j][i] := t[3]:
    end do:
end do:</Text-field>
</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L3899" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">A := &lt;seq(&lt;x[1][i]*x[2][i]|y[1][i]*x[2][i]|z[1][i]*x[2][i]|x[1][i]*y[2][i]|y[1][i]*y[2][i]|z[1][i]*y[2][i]|x[1][i]*z[2][i]|y[1][i]*z[2][i]|z[1][i]*z[2][i]&gt;, i=1..nP)&gt;:
NS := NullSpace(A):
X := map(x-&gt;&lt;&lt;NS[x][1]|NS[x][2]|NS[x][3]&gt;, &lt;NS[x][4]|NS[x][5]|NS[x][6]&gt;, &lt;NS[x][7]|NS[x][8]|NS[x][9]&gt;&gt;, [$ 1..2]):
F := u*X[1]+X[2]:
K := &lt;&lt;f|0|a&gt;, &lt;0|f|b&gt;, &lt;0|0|1&gt;&gt;:
fu := solve(Determinant(F), u)[1]:
fF := subs(u=fu, F):</Text-field>
</Input>
</Group>
<Group hide-input="false" hide-output="false" labelreference="L3902" drawlabel="true" applyint="true" applyrational="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">omega := algsubs(f^2=p, K.K^%T):
G := map(factor, 1/2*Trace(fF.omega.fF^%T.omega)*fF-fF.omega.fF^%T.omega.fF):
eq1 := factor(1/2*(tau^2-1)*Trace(fF.omega.fF^%T.omega)+(tau+1)*Trace(omega.fF.omega.fF)-tau*Trace(omega.fF)^2):
eqs := [seq(seq(G[i, j], i=1..3), j=1..3), eq1]:
vars := indets(eqs):
tord := tdeg(op(vars)):
uu := [a, b, p]:
tord := tdeg(op(uu)):
eqs0 := [eqs[1], eqs[5], eqs[9]+eqs[8], eqs[10]]:
mm := [a^3*b, a^2*b^2, a*b^3, a^2*b, a^3*p, a^2*b*p, a*b^2*p, a^4, b^4, a^3, a*b^2, b^3, a^2*p, b^2*p^2, a*p^2, a*b*p, b^2*p, b^3*p, a^2, a*b*p^2, a^2*p^2, a*b, b^2, b*p^2, p^3, a*p, b*p, p^2, a, b, p, 1]:
mm0 := subsop(5=NULL, 6=NULL, 7=NULL, 14=NULL, 15=NULL, 18=NULL, 20=NULL, 21=NULL, 24=NULL, 25=NULL, mm):
sA0 := convert(mm0, Vector):
for k from 1 to nops(eqs0) do
    cffs||k := map(x-&gt;remove(has, expand(eqs0[k]/x), uu), sA0):
end do:
A0 := &lt;seq(Transpose(cffs||k), k=1..nops(eqs0))&gt;:
tA0 := ReducedRowEchelonForm(A0):
eqs1 := [seq(Row(tA0, i).sA0, i=1..4), Row(tA0, 4).sA0*a, Row(tA0, 4).sA0*b, Row(tA0, 4).sA0*p]:
mm1 := subsop(20=NULL, mm):
sA1 := convert(mm1, Vector):
for k from 1 to nops(eqs1) do
    cffs||k := map(x-&gt;remove(has, expand(eqs1[k]/x), uu), sA1):
end do:
A1 := &lt;seq(Transpose(cffs||k), k=1..nops(eqs1))&gt;:
tA1 := ReducedRowEchelonForm(A1):
eqs2 := [seq(Row(tA1, i).sA1, i in [1, 2, 3, 4]), seq(factor(Row(tA1, i).sA1/p), i in [5, 6, 7])]:</Text-field>
</Input>
