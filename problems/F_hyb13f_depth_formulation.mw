<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### compute coefficient matrix of initial polynomial system
cat(C_, prob) := proc({symbolic:=1})
    _name, _size := [`A`], [[15,1]]:
    
    if symbolic=1 then
        _A := [seq(Matrix(op(_size[i]), (x,y)-&gt;_name[i](x+(y-1)*_size[i][1])), i=1..nops(_name))]:
    else
        _A := [seq(rndMat(op(_size[i])), i=1..nops(_name))]:
    end if:
    
    ################## place your code for constructing polynomials here
    skewsymmmat := proc(v)
        local X;
        X := convert([[0, -v[3, 1], v[2, 1]], [v[3, 1], 0, -v[1, 1]], [-v[2, 1], v[1, 1], 0]], Matrix);
        return X;
    end proc:
    quat2rotmat := proc(quat)
        local temp, R, v, s;
        v := convert([[quat[2]], [quat[3]], [quat[4]]], Matrix);
        s := quat[1];
        temp := Typesetting:-delayDotProduct(LinearAlgebra[Transpose](v), v);
        R := 2*Typesetting:-delayDotProduct(v, LinearAlgebra[Transpose](v)) - 2*s*skewsymmmat(v) + (s^2 - temp[1, 1])*LinearAlgebra[IdentityMatrix](3);
        return R/(v[1, 1]^2 + v[2, 1]^2 + v[3, 1]^2 + s^2);
    end proc: 
    K := LinearAlgebra[DiagonalMatrix]([f, f, 1]);
    R := quat2rotmat([1, r1, r2, r3]);
    noX := 4;
    T := Matrix(3, 1, symbol = t);
    centers := Matrix(3, noX, symbol = c);
    Q := Matrix(3, noX, symbol = q);
    P := Matrix(3, noX, symbol = p);
    P[() .. (), 1] := convert([[0], [0], [1]], Matrix);
    Q[() .. (), 1] := convert([[1], [0], [1]], Matrix);
    Q[3, () .. ()] := 1;
    centers[() .. (), 1 .. 4] := 0;

    alpha := LinearAlgebra[DiagonalMatrix](map(i -&gt; cat('a',i), [seq(1 .. noX)]));
    beta := LinearAlgebra[DiagonalMatrix](map(i -&gt; cat('b',i), [seq(1 .. noX)]));
    beta[2, 2] := 1;
    beta[3, 3] := 1;
    beta[4, 4] := 1;

    XQ := ((LinearAlgebra[MatrixInverse](K)) . Q) . alpha;
    XP := (R . ((P . beta) + centers)) +~ (T . (Matrix(1, noX, fill = 1))); 
    E := [];
    temp := [];
    for comb in combinat[choose](noX, 2) do
        temp := [op(temp), XP[() .. (), comb[1]] - XP[() .. (), comb[2]] = XQ[() .. (), comb[1]] - XQ[() .. (), comb[2]]];
        E := [op(E), numer(simplify((LinearAlgebra[Transpose](lhs(temp[-1]))) . (lhs(temp[-1]))) - simplify((LinearAlgebra[Transpose](rhs(temp[-1]))) . (rhs(temp[-1]))))];
    end do;
    E1 := [];
    for comb in combinat[choose](numelems(temp), 2) do
        E1 := [op(E1), numer(simplify((LinearAlgebra[Transpose](lhs(temp[comb[1]]))) . (lhs(temp[comb[2]]))) - simplify((LinearAlgebra[Transpose](rhs(temp[comb[1]]))) . (rhs(temp[comb[2]]))))];
    end do;
    E := map(e -&gt; numer(simplify(e)), subs([f = sqrt(f)], E));
    E1 := map(e -&gt; numer(simplify(e)), subs([f = sqrt(f)], E1));
    eqs := [op(E1),op(E)]: 
    eqs := subs([op([p[1, 2], p[1, 3], p[1, 4], p[2, 2], p[2, 3], p[2, 4], p[3, 2], p[3, 3], p[3, 4], q[1, 2], q[1, 3], q[1, 4], q[2, 2], q[2, 3], q[2, 4]] =~ map(i -&gt; A[i], [seq(1 .. 15)]))], eqs):
    eqs := subs([op(map(i-&gt;A[i], [seq(1..15)]) =~ convert(_A[1], list,nested=false))], eqs):  
    eqs := map(e -&gt; expand(simplify(e)), [op(eqs)]):
    ##################

    _F := map(expand, eqs):
    if symbolic=0 then return _F: end if:
    _vars := indets(_F) minus indets(_A):
    _C0, _V0 := poly2matrix(_F, _vars):
    _V := srt(_V0, tdeg(_vars[])):
    [permuteColumns(_C0, _V0, _V)], _V, _name, _size:
end proc:</Text-field>
