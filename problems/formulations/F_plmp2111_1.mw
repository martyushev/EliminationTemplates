<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nP, nL, nV := 4, 0, 3:
irnd := rand(-1000..1000)/1000:
# make projections
# scene points
for i from 1 to 3 do
    Q[i] := &lt;irnd(), irnd(), irnd()&gt;:
end do:
Q[4] := Q[1] + irnd()*(Q[2] - Q[1]):
#Q[5] := Q[1] + irnd()*(Q[3] - Q[1]):

rP1 := &lt;IdentityMatrix(3)|ZeroVector(3)&gt;:
for j from 1 to nV do
    rt||j := Vrnd():
    rR||j := getRotMatrix(Vrnd()):
    rP||j := &lt;rR||j|rt||j&gt;:
end do:

rE12 := getFundMatrix(P1, P2):

# make projections
for j from 1 to nV do
    for i from 1 to nP do
        x[j][i] := (rP||j.&lt;Q[i], 1&gt;)[1, 1]:
        y[j][i] := (rP||j.&lt;Q[i], 1&gt;)[2, 1]:
        z[j][i] := (rP||j.&lt;Q[i], 1&gt;)[3, 1]:
        #print(z[j][i]):
    end do:
end do:</Text-field>
</Input>
</Group>
