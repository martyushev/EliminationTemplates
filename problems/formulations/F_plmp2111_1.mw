<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">eqs := {}:
ind2 := combinat[choose]([$ 1..nP], 2):
ind4 := combinat[choose]([$ 1..(nP-1)*nV], 4):
cind := combinat[choose]([$ 1..nV], 3):
rind := combinat[choose]([$ 1..4], 3):
for kl in ind2 do
    k, l := op(kl):
    Nkl := seq(cross(&lt;x[j][k], y[j][k], z[j][k]&gt;).&lt;x[j][l], y[j][l], z[j][l]&gt;, j=1..nV):
    M := Matrix([seq(Transpose(P||j).Nkl[j], j=1..nV)]):
    #print(M):
    eqs := {op(eqs), seq(seq(numer(factor(Determinant(SubMatrix(M, r, c)))), r in rind), c in cind)}:
    #print(nops(eqs)):
end do:
for p from 1 to nP do
    mm := []:
    for kl in select(has, ind2, p) do
        k, l := op(kl):
        Nkl := seq(cross(&lt;x[j][k], y[j][k], z[j][k]&gt;).&lt;x[j][l], y[j][l], z[j][l]&gt;, j=1..nV):
        mm := [op(mm), seq(Transpose(P||j).Nkl[j], j=1..nV)]:
    end do:
    M := Matrix(mm):
    #print(op(1, M)):
    eqs := {op(eqs), seq(numer(Determinant(SubMatrix(M, 1..-1, c))), c in ind4)}:
    print(nops(eqs)):
end do:</Text-field>
