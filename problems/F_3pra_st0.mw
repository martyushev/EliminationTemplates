<Group hide-output="false" labelreference="L4568" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">R := (2*s^2-1)*IdentityMatrix(3)+2*(&lt;u, v, w&gt;.&lt;u, v, w&gt;^%T-s*cross(&lt;u, v, w&gt;)):
jj := [2, 3, 1]:
kk := [3, 1, 2]:
for i from 1 to 3 do
    j, k := jj[i], kk[i]:
    P1 := cross(X1[..,i]).X1[..,j]:
    P2 := cross(X2[..,i]).X2[..,j]:
    Q1 := cross(X1[..,i]).X1[..,k]:
    Q2 := cross(X2[..,i]).X2[..,k]:
    eq[i] := Determinant(&lt;&lt;P2^%T.R.X1[..,j]|X2[..,j]^%T.R.P1&gt;, &lt;Q2^%T.R.X1[..,k]|X2[..,k]^%T.R.Q1&gt;&gt;):
    eq1[i] := Determinant(&lt;&lt;X2[..,j]^%T.R.P1|P2^%T.R.X1[..,j]&gt;, &lt;&lt;u, v, w&gt;^%T.X1[..,i]|-&lt;u, v, w&gt;^%T.X2[..,i]&gt;&gt;):
end do:
eqs := [seq(eq[i], i=1..3), seq(eq1[i], i=1..3), u^2+v^2+w^2+s^2-1]:</Text-field>
</Input>
</Group>
