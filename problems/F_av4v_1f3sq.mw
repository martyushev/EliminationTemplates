<Group view="code" labelreference="L4635" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">### compute coefficient matrix of initial polynomial system
cat(C_, prob) := proc({symbolic:=1})
    _name, _size := [`P1`, `P2`, `P3`, `P4`], [[3,4] $ 4]:
    if symbolic=0 then
        _A := [seq(rndMat(_size[i][]), i=1..nops(_name))]:
    else
        _A := [seq(Matrix(_size[i][], (x,y)-&gt;_name[i](x+(y-1)*_size[i][1])), i=1..nops(_name))]:
    end if:
    
    ################## place your code for constructing polynomials here
    for i from 1 to 4 do
        for j from 1 to 4 do
            F||i||j := getFundMatrix(_A[j], _A[i]):
        end do:
    end do:
    C||1 := LinearAlgebra[DiagonalMatrix]([c111,c111,c133]):
    for k from 2 to 4 do
        C||k := Matrix(3, shape=symmetric, (x,y)-&gt;c||k||x||y):
    end do:
    dm := (A, B)-&gt;(LinearAlgebra[Trace](A.B) - LinearAlgebra[Trace](A)*LinearAlgebra[Trace](B))*LinearAlgebra[IdentityMatrix](3) + A*LinearAlgebra[Trace](B) + B*LinearAlgebra[Trace](A) - A.B - B.A:
    indx := combinat[choose](4, 3):
    for m from 1 to nops(indx) do
        i, j, k := indx[m][]:
        eq||m := C||k.F||j||k^+.LinearAlgebra[Adjoint](F||i||j) + LinearAlgebra[Adjoint](F||j||k).F||i||j^+.C||i + dm(F||i||j.C||j.F||j||k, F||i||k):
    end do:
    lineqs := map(expand, [seq(convert(eq||m, list)[], m=1..4)]):
    vars := [indets(lineqs)[]]:
    tord := tdeg(vars[]):
    A := convert(map(x-&gt;&lt;map(y-&gt;coeff(x, y), vars)&gt;, lineqs), Matrix)^+:
    G := &lt;seq(LinearAlgebra[Column](convert([LinearAlgebra[NullSpace](A)[]], Matrix).Matrix(6, (x,y)-&gt;rnd()), i)^+, i=1..6)&gt;:
    NS := [seq(LinearAlgebra[Row](G, i)^+, i=1..6)]:
    n := nops(NS):
    uu := [seq(u||k, k=1..n)]:
    cc1, cc2, cc3, cc4 := Matrix(3, shape=symmetric), Matrix(3, shape=symmetric), Matrix(3, shape=symmetric), Matrix(3, shape=symmetric):
    cc1[1, 1] := add(NS[i][1]*uu[i], i=1..n):
    cc1[2, 2] := cc1[1, 1]:
    cc1[3, 3] := add(NS[i][2]*uu[i], i=1..n):
    for k from 2 to 4 do
        cc||k[1, 1] := add(NS[i][3 + 6*(k-2)]*uu[i], i=1..n):
        cc||k[1, 2] := add(NS[i][4 + 6*(k-2)]*uu[i], i=1..n):
        cc||k[1, 3] := add(NS[i][5 + 6*(k-2)]*uu[i], i=1..n):
        cc||k[2, 2] := add(NS[i][6 + 6*(k-2)]*uu[i], i=1..n):
        cc||k[2, 3] := add(NS[i][7 + 6*(k-2)]*uu[i], i=1..n):
        cc||k[3, 3] := add(NS[i][8 + 6*(k-2)]*uu[i], i=1..n):
    end do:
    q||4 := 1:
    for i from 2 to 4 do
        eqx||i[1] := q||i^2 - q||i*cc||i[1, 1] - q||i*cc||i[2, 2] + cc||i[1, 1]*cc||i[2, 2] - cc||i[1, 2]^2;
        eqx||i[2] := q||i*cc||i[1, 3] + cc||i[1, 2]*cc||i[2, 3] - cc||i[1, 3]*cc||i[2, 2];
        eqx||i[3] := q||i*cc||i[2, 3] - cc||i[1, 1]*cc||i[2, 3] + cc||i[1, 2]*cc||i[1, 3];
        eqx||i[4] := q||i*cc||i[3, 3] - cc||i[1, 1]*cc||i[3, 3] + cc||i[1, 3]^2;
        eqx||i[5] := cc||i[1, 3]*cc||i[2, 3] - cc||i[1, 2]*cc||i[3, 3];
        eqx||i[6] := cc||i[1, 1]*cc||i[3, 3] - cc||i[1, 3]^2 - cc||i[2, 2]*cc||i[3, 3] + cc||i[2, 3]^2;
    end do:
    eqs := [seq(eqx2[k], k=5..5), seq(eqx3[k], k=1..6), seq(eqx4[k], k=1..6)]:
    ##################

    _F := map(expand, eqs):
    if symbolic=0 then return _F: end if:
    _vars := indets(_F) minus indets(_A):
    _C0, _V0 := poly2matrix(_F, _vars):
    _V := srt(_V0, tdeg(_vars[])):
    [permuteColumns(_C0, _V0, _V)], _V, _name, _size:
end proc:</Text-field>
